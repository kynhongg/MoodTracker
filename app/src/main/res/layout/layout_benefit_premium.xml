        android:src="@drawable/ic_item_pre_6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/line5" />

    <TextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="15dp"
        android:fontFamily="@font/nunito_medium_500"
        android:text="@string/txt_item_pre_6"
        android:textColor="@color/text_color"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@id/imgPre6"
        app:layout_constraintEnd_toStartOf="@id/imgPre6"
        app:layout_constraintStart_toEndOf="@id/tvPre6"
